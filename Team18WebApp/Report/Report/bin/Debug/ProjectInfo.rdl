      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Proj_id">
            <Value>=Parameters!Proj_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  projects.PROJECT_NUM
  ,projects.PROJECT_DEPT
  ,projects.project_name
  ,tasks.project_id
  ,tasks.task_id
  ,tasks.task_budget
  ,tasks.task_name
  ,tasks.task_deadline
  ,tasks.task_status
  ,tasks.task_expenses
FROM
  team18.projects AS projects
  INNER JOIN team18.tasks AS tasks
    ON projects.PROJECT_NUM = tasks.project_id
WHERE 
  projects.PROJECT_NUM IN (@Proj_id)
  </CommandText>
